.box{
  margin: 0;
  padding: 0px 5px;
  background-color: #FFFFFF;
}
.content {
  text-align: center;
}

.mt-10 {
  margin-top: 10px;
}

.date-t {
  border-radius: 4px;
  border:1px solid #ff5722
}

.date-y {
  border-radius: 4px;
  border:1px solid #5fb878
}

.t_date .title {
  background-color: #ff5722;
  /* border-radius: 4px 4px 0px 0px; */
  height: 40px;
  line-height: 40px;
  color: #fff;
}

.y_date .title {
  background-color: #5fb878;
  /* border-radius: 4px 4px 0px 0px; */
  height: 40px;
  line-height: 40px;
  color: #fff;
}

.count {
  min-height: 100px;
  text-align: left;
  padding: 5px 0px 5px 10px;
}

.count>.list {
  margin: 5px auto;
  font-size: 20px;
  height: 30px;
  line-height: 30px;
  border-left: 5px solid #0078ad;
  padding-left: 5px;
}

.lines {
  font-size: 30px;
  color: #ff5722;
  padding: 2px;
  font-weight: bold;
}
