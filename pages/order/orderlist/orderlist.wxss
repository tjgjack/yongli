.listbox{
  width: 100%;
  height: auto;
  font-size: 15px;
    }

.item{
  display: flex;
  flex-direction: row;
  margin: 5px;
  /* background-color: #dddd; */
  height: 50px;
  padding-bottom: 5px;
  border-bottom: 1px solid #e8e8e8;
  justify-content: space-between;
  }

.icondate{
  height: 50px;
  width: 50px;
  font-size: 25px;
  line-height: 50px;
  text-align: center;
  background-color: #6cd9d9;
  color:#FFF;
  margin-right: 10px;
  border-radius: 2px;
  flex-shrink: 0;
  flex-grow: 0;
}

.connet{
  width: 100%;
  height: 50px;
  line-height: 50px;
  display: flex;
  flex-direction: column;
  /* 内容背景颜色 */
  /* background-color: #DDD; */
}

.title{
  /* background-color: red; */
  font-size: 15px;
  line-height: 25px;
  height: 25px;
  /* flex-shrink: 0;
  flex-grow: 0; */
}

.vip{
  background-color: #0084b4;
  color: #FFF;
  font-size: 15px;
  /* line-height: 30px; */
  height: 30px;
  padding: 0 5px;
  border-radius: 2px;
  padding-bottom:1px;

}

.list{
  /* background-color: blue; */
  height: 30px;
  font-size: 15px;
  line-height: 30px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.list text{
  margin-right: 10px;
}

.icon{
  height: 50px;
  line-height: 50px;
  font-size: 25px;
  color: #DDD;
  /* 箭头背景颜色 */
  /* background-color: greenyellow; */
}

.sb{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
